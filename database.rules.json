{
  "rules": {
    ".read": "auth != null",
    ".write": false,
    "permissions": {
      ".read": "root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'",
      ".write": "root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'"
    },
    "roles": {
      ".write": "root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'"
    },
    "modules": {
      ".write": "root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'"
    },
    "auditLogs": {
      ".read": "root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'",
      ".write": "auth != null"
    },
    "cobranzas": {
      "configuracion": {
        ".write": "root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'"
      },
      "$uid": {
        ".read": "$uid === auth.uid || root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'",
        ".write": "root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'"
      }
    },
    "users": {
      "$uid": {
        ".read": "auth.uid === $uid || root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'",
        ".write": "auth.uid === $uid",
        ".indexOn": ["empadronadoId"]
      }
    },
    "empadronados": {
      "$empadronadoId": {
        ".read": "data.child('authUid').val() === auth.uid || root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'",
        ".write": "data.child('authUid').val() === auth.uid || root.child('users').child(auth.uid).child('roleId').val() === 'presidencia' || root.child('users').child(auth.uid).child('roleId').val() === 'administrador'",
        ".validate": "newData.hasChildren(['dni', 'numeroPadron', 'authUid'])"
      },
      ".indexOn": ["authUid", "dni", "numeroPadron"]
    },
    "cobranzas_v2": {
      "comprobantes": {
        ".write": "auth != null"
      }
    },
    "personal-seguridad": {
      ".write": "auth != null"
    },
    "eventos": {
      "$eventoId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "inscripcionesEventos": {
      "$inscripcionId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "finanzas": {
      "$movimientoId": {
        ".write": "auth != null"
      }
    },
    "acceso": {
      "visitas": {
        ".write": "auth != null",
        ".indexOn": ["estado", "patronId", "createdAt", "dni"]
      },
      "trabajadores": {
        ".write": "auth != null",
        ".indexOn": ["estado", "dni", "padron"]
      },
      "proveedores": {
        ".write": "auth != null",
        ".indexOn": ["estado", "ruc"]
      },
      "maestrosObra": {
        ".write": "auth != null"
      },
      "favoritos": {
        ".write": "auth != null"
      }
    },
    "seguridad": {
      "porticos": {
        ".write": "auth != null"
      }
    }
  }
}